   {
    "name": "denebsoft/yii2-adldap-module",
    "description": "yii2 adldap extention (wrapper for adldap/adLDAP Module)",
    "keywords": ["active directory","ldap","yii2", "adldap", "ad"],
    "type": "yii2-extension",
     "license": "LGPL-2.1",
    "support": {
        "issues": "https://github.com/deneb-air/yii2-adldap-module/issues",
        "source": "https://github.com/deneb-air/yii2-adldap-module"
    },
    "authors": [
        {
            "name": "Vitaliy"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adldap/adLDAP.git"
        }
    ],
    "require": {
        "adldap/adldap": "dev-master"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "TBA"
    },
    "autoload": {
        "psr-4" : {
         "denebsoft\\" : "src/"
     }
    }
}
